<script>function checkPhoto(){$("#photocont").empty(),$("#photocont").append($("#gf").width()>600?'<img src="http://c.o0bg.com/rw/Boston/2011-2020/WebGraphics/Politics/BostonGlobe.com/2014/07/25cappoll/Poll0725_2000px.png">':'<img src="http://c.o0bg.com/rw/Boston/2011-2020/WebGraphics/Politics/BostonGlobe.com/2014/07/25cappoll/Poll0725_600px.png">')}globe.graphic=function(){checkPhoto(),$(window).resize(function(){checkPhoto()})},globe.onDefine('window.jQuery && $("#gf").length',function(){globe.graphic()});</script>
